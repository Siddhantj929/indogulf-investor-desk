    <link rel="stylesheet" href="/assets/vendors/toolbar/css/toolbar.css">
    <style>
        .text-justify {
            text-align: justify;
        }

        .cat-sidebar {}

        .cat-group {
            margin-top: 12px;
        }

        .cat-group,
        .cat-children {
            display: flex;
            flex-direction: column;
            justify-content: flex-start;
            align-items: flex-end;
        }

        .cat-children {
            width: 93%;
            align-items: flex-start;
        }

        .cat-link {
            display: block;
            width: 100%;
            padding: 6px 12px;
            border-radius: 4px;
            box-sizing: border-box;
            font-size: 0.94rem;
            line-height: 1.4;
            margin-top: 4px;
            transition: all 120ms linear;
        }

        .cat-link.active,
        .cat-link:hover {
            background: #F9F9F9;
            color: var(--agriox-primary, #334b35);
        }

        .cat-link.active {
            border: 1px solid #E8E8E8;
        }

        .cat-link.parent {
            font-weight: 500;
        }

        .cat-files-grid {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 12px;
        }

        .cat-file {
            border: 1px solid #E8E8E8;
            background: #F9F9F9;
            padding: 16px;
            border-radius: 8px;
            display: flex;
            gap: 12px;
            align-items: center;
            transition: all 120ms linear;
        }

        .cat-file:hover {
            color: inherit;
            background: #f4f4f4;
        }

        .cat-file img {
            height: 40px;
        }

        .cat-file div {
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: flex-start;
            height: 100%;
        }

        .cat-file h6 {
            margin-bottom: 4px;
        }

        .cat-file small {
            line-height: 1;
            font-size: 0.8rem;
        }

        @media (min-width: 992px) {
            .cat-files-grid {
                display: grid;
                grid-template-columns: repeat(3, 1fr);
                gap: 12px;
            }
        }

        @media (max-width: 576px) {
            .cat-files-grid {
                display: grid;
                grid-template-columns: repeat(1, 1fr);
                gap: 12px;
            }

            .cat-sidebar {
                border-bottom: 1px solid #dee2e6 !important;
                padding-bottom: 24px;
                margin-bottom: 24px;
            }
        }

        .loginPage {
            height: 100vh;
            width: 100vw;
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
        }

        .login-form {
            width: 100%;
            max-width: 500px;
            z-index: 999;
        }
    </style>
</head>

<body>

    <div class="preloader">
        <img class="preloader__image" width="200" src="/assets/images/loader.png" alt="" />
    </div>
    <!-- /.preloader -->
    <div class="page-wrapper">
        <div class="loginPage">
            <div class="logo my-5 mx-auto text-center">
                <a href="https://groupindogulf.com/" class="text-center">
                    <img class="dark-logo" src="/assets/images/resources/HeaderLogo.png" alt="" />
                </a>
            </div>
            <!-- /.sticky-header__content -->
            <div class="container" style="min-height: 500px;">
                <form class="card p-3 shadow login-form mx-auto" method="post" action="/login">
                    <div class="card-body">
                        <h5 class="card-title">Login to Admin Portal</h5>
                        <p class="card-subtitle mb-4 text-body-secondary">Please enter your username and password
                        </p>
                        <div class="mb-3">
                            <label for="username" class="form-label">Username</label>
                            <input type="text" class="form-control" name="username" id="username"
                                placeholder="Username">
                        </div>
                        <div class="mb-3">
                            <label for="password" class="form-label">Password</label>
                            <input type="password" name="password" id="password" class="form-control"
                                aria-describedby="password">
                        </div>
                        <div class="d-grid gap-2 mt-4 mb-3">
                            <button type="submit" class="btn btn-success">Confirm identity</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!--Page Header End-->
    </div>

    <a href="#" data-target="html" class="scroll-to-target scroll-to-top"><i class="icon-right-arrow-2"></i></a>

