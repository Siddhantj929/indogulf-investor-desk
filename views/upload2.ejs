    <script src="/assets/js/agriox.js"></script>
    <script>
        $('#uploadForm').submit(function (e) {
            e.preventDefault();
            var formData = new FormData(this);
            // Log formData entries
            for (var pair of formData.entries()) {
                console.log(pair[0] + ': ' + pair[1]);
            }
            var $progress = $('.progress');
            var $progressBar = $('.progress-bar');
            var $uploadButton = $('#uploadButton');
            var $updateButton = $('#updateButton');
            var $cancelEditButton = $('#cancelEditButton');
            var isUpdate = $updateButton.is(':visible');
            if (isUpdate) {
                $.ajax({
                    url: '/upload/update',
                    type: 'POST',
                    data: formData,
                    contentType: false,
                    processData: false,
                    success: function (data) {
                        window.location.reload();
                    },
                    error: function (xhr, status, error) {
                        console.error('Error updating PDF:', error);
                    }
                });
            } else {
                $uploadButton.hide();
                $progress.show();
                $.ajax({
                    url: '/upload',
                    type: 'POST',
                    data: formData,
                    contentType: false,
                    processData: false,
                    xhr: function () {
                        var xhr = new window.XMLHttpRequest();
                        xhr.upload.addEventListener('progress', function (evt) {
                            if (evt.lengthComputable) {
                                var percentComplete = evt.loaded / evt.total;
                                percentComplete = Math.round(percentComplete * 100);
                                $progressBar.css('width', percentComplete + '%').attr('aria-valuenow', percentComplete).text(percentComplete + '%');
                            }
                        }, false);
                        return xhr;
                    },
                    success: function (data) {
                        window.location.reload();
                    },
                    error: function (xhr, status, error) {
                        console.error('Error uploading PDF:', error);
                    }
                });
            }
        });
        // Handle delete button click event
        $(document).on('click', '.delete-btn', function () {
            var pdfId = $(this).data('id');
            var confirmDelete = confirm("Are you sure you want to delete this PDF?");
            if (confirmDelete) {
                $.ajax({
                    url: '/upload/delete',
                    type: 'POST',
                    data: { id: pdfId },
                    success: function (response) {
                        // Handle successful deletion (e.g., reload the page or remove the item from the DOM)
                        window.location.reload();
                    },
                    error: function (xhr, status, error) {
                        // Handle error
                        alert("An error occurred while deleting the PDF. Please try again.");
                    }
                });
            }
        });
        // Enable tooltips
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
            $(".cat-children .cat-link").each(function () {
                let collapseId = $(this).attr('id').replace('subcategory', 'collapse')
                let collapseItem = $(`#${collapseId}`);
                $(this).width($(".cat-children").width() - 48);
                collapseItem.width($(".cat-children").width() - 48);
                console.log($(this).data('has-pdfs'))
                if (!$(this).data('has-pdfs'))
                    collapseItem[0].style.setProperty('border', 'none', 'important');
            });
            // Toggle chevron icon based on collapse state
            $('.collapse').on('shown.bs.collapse', function () {
                $(this).parent().find('.bi').removeClass('bi-chevron-down').addClass('bi-chevron-up');
            }).on('hidden.bs.collapse', function () {
                $(this).parent().find('.bi').removeClass('bi-chevron-up').addClass('bi-chevron-down');
            });
            // Handle edit button click event
            $(document).on('click', '.edit-file', function () {
                var pdfId = $(this).data('id');
                var pdfElement = $('#pdf_' + pdfId);
                var pdfName = pdfElement.data('name');
                var isPublic = pdfElement.data('is-public');
                var isPrintable = pdfElement.data('is-printable');
                var isDownloadable = pdfElement.data('is-downloadable');
                var isDisclaimer = pdfElement.data('is-disclaimer');
                var categoryId = pdfElement.data('category-id');
                $('#pdfId').val(pdfId);
                $('#categorySelect').val(categoryId);
                $('#pdfInputGroup').hide();
                $('#pdfInput').removeAttr('required'); // Remove required attribute
                $('#isPublic').prop('checked', isPublic);
                $('#isPrintable').prop('checked', isPrintable);
                $('#isDownloadable').prop('checked', isDownloadable);
                $('#isDisclaimer').prop('checked', isDisclaimer);
                $('#fileName').text(pdfName);
                $('#fileNameDisplay').show();
                $('#uploadButton').hide();
                $('#updateButton').show();
                $('#cancelEditButton').show();
            });
            // Handle cancel edit button click event
            $('#cancelEditButton').click(function () {
                $('#pdfId').val('');
                $('#pdfInputGroup').show();
                $('#pdfInput').attr('required', true); // Add required attribute back
                $('#isPublic').prop('checked', false);
                $('#isPrintable').prop('checked', false);
                $('#isDownloadable').prop('checked', false);
                $('#isDisclaimer').prop('checked', false);
                $('#fileNameDisplay').hide();
                $('#uploadButton').show();
                $('#updateButton').hide();
                $('#cancelEditButton').hide();
            });
        });
    </script>
</body>
</html>