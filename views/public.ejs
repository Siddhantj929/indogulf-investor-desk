        <script>
            document.querySelector(".home-link").classList.remove("current");

            // Dropdown functionality
            document.addEventListener("DOMContentLoaded", function () {
                // Check if any child category is active and expand its parent
                const activeChild = document.querySelector(".cat-children .cat-link.active");
                if (activeChild) {
                    const parentGroup = activeChild.closest(".cat-group");
                    if (parentGroup) {
                        const dropdownToggle = parentGroup.querySelector(".cat-dropdown-toggle");
                        const childrenContainer = parentGroup.querySelector(".cat-children");
                        if (dropdownToggle && childrenContainer) {
                            dropdownToggle.classList.add("expanded");
                            childrenContainer.classList.add("expanded");
                        }
                    }
                }

                // Also check if parent category is active and has children
                const activeParent = document.querySelector(".cat-parent-wrapper .cat-link.parent.active");
                if (activeParent) {
                    const parentWrapper = activeParent.closest(".cat-parent-wrapper");
                    if (parentWrapper) {
                        const dropdownToggle = parentWrapper.querySelector(".cat-dropdown-toggle");
                        const parentGroup = parentWrapper.closest(".cat-group");
                        const childrenContainer = parentGroup ? parentGroup.querySelector(".cat-children") : null;
                        if (dropdownToggle && childrenContainer) {
                            dropdownToggle.classList.add("expanded");
                            childrenContainer.classList.add("expanded");
                        }
                    }
                }

                // Handle dropdown toggle
                document.querySelectorAll(".cat-dropdown-toggle").forEach(function (toggle) {
                    toggle.addEventListener("click", function (e) {
                        e.preventDefault();
                        e.stopPropagation();

                        const categoryName = this.getAttribute("data-category");
                        const childrenId = "children-" + categoryName.replace(/\s+/g, "-").toLowerCase();
                        const childrenContainer = document.getElementById(childrenId);

                        if (childrenContainer) {
                            // Toggle the expanded class
                            this.classList.toggle("expanded");
                            childrenContainer.classList.toggle("expanded");
                        }
                    });
                });
            });

            // Disclaimer modal functionality
            let currentViewUrl = "";
            $(document).ready(function () {
                // Handle click on PDF file links
                $(".cat-file").on("click", function (e) {
                    const isDisclaimer = $(this).data("pdf-disclaimer");
                    const pdfPath = $(this).data("pdf-path");
                    const pdfName = $(this).data("pdf-name");
                    // If disclaimer is required, prevent default and show modal
                    if (isDisclaimer == 1) {
                        e.preventDefault();
                        currentViewUrl = "/view?file=" + encodeURIComponent(pdfPath);
                        // Update modal title with file name
                        $("#disclaimerModalLabel").text("Disclaimer - " + pdfName);
                        // Show the modal
                        $("#disclaimerModal").modal("show");
                    }
                    // If no disclaimer required, let the normal link behavior proceed
                });
                // Handle agree and view button
                $("#agreeAndView").on("click", function () {
                    if (currentViewUrl) {
                        // Open the PDF viewer in a new tab
                        window.open(currentViewUrl, "_blank");
                        // Close the modal
                        $("#disclaimerModal").modal("hide");
                        // Reset the view URL
                        currentViewUrl = "";
                    }
                });
                // Reset when modal is closed
                $("#disclaimerModal").on("hidden.bs.modal", function () {
                    currentViewUrl = "";
                    $("#disclaimerModalLabel").text("Important Disclaimer");
                });
            });
        </script>
