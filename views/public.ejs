        <style>
            .text-justify {
                text-align: justify;
            }
            .cat-sidebar {
            }
            .cat-group {
                margin-top: 12px;
            }
            .cat-group,
            .cat-children {
                display: flex;
                flex-direction: column;
                justify-content: flex-start;
                align-items: flex-end;
            }
            .cat-children {
                width: 93%;
                align-items: flex-start;
                max-height: 0;
                overflow: hidden;
                transition: max-height 0.3s ease-out;
            }
            .cat-children.expanded {
                max-height: 500px;
                transition: max-height 0.3s ease-in;
            }
            .cat-link {
                display: block;
                width: 100%;
                padding: 6px 12px;
                border-radius: 4px;
                box-sizing: border-box;
                font-size: 0.94rem;
                line-height: 1.4;
                margin-top: 4px;
                transition: all 120ms linear;
                position: relative;
            }
            .cat-link.active,
            .cat-link:hover {
                background: #f9f9f9;
                color: var(--agriox-primary, #334b35);
            }
            .cat-link.active {
                border: 1px solid #e8e8e8;
            }
            .cat-link.parent {
                font-weight: 500;
            }
            .cat-parent-wrapper {
                display: flex;
                align-items: center;
                gap: 4px;
                width: 100%;
                margin-top: 4px;
            }
            .cat-parent-wrapper .cat-link.parent {
                flex: 1;
                margin-top: 0;
                border-radius: 4px 0 0 4px;
            }
            .cat-dropdown-toggle {
                display: flex;
                align-items: center;
                justify-content: center;
                width: 32px;
                height: 34px !important;
                background: transparent;
                border: none;
                border-radius: 0 4px 4px 0;
                cursor: pointer;
                transition: all 120ms linear;
                padding: 6px 0;
            }
            .cat-dropdown-toggle:hover {
                background: #f9f9f9;
            }
            .cat-dropdown-toggle i {
                font-size: 0.75rem;
                color: #666;
                transition: transform 0.3s ease;
            }
            .cat-dropdown-toggle.expanded i {
                transform: rotate(180deg);
            }
            .cat-parent-wrapper:has(.cat-link.active) .cat-dropdown-toggle {
                background: #f9f9f9;
                border: 1px solid #e8e8e8;
                border-left: none;
                margin-left: -1px;
            }
            .cat-parent-wrapper:hover .cat-link.parent,
            .cat-parent-wrapper:hover .cat-dropdown-toggle {
                background: #f9f9f9;
            }
            .cat-parent-wrapper .cat-link.parent {
                border-radius: 4px;
            }
            .cat-parent-wrapper:has(.cat-dropdown-toggle) .cat-link.parent {
                border-radius: 4px 0 0 4px;
                margin-right: -1px;
            }
            .cat-dropdown-toggle {
                border-radius: 0 4px 4px 0;
                border-left: 1px solid #e8e8e8;
            }
            .cat-files-grid {
                display: grid;
                grid-template-columns: repeat(2, 1fr);
                gap: 12px;
            }
            .cat-file {
                border: 1px solid #e8e8e8;
                background: #f9f9f9;
                padding: 16px;
                border-radius: 8px;
                display: flex;
                gap: 12px;
                align-items: center;
                transition: all 120ms linear;
                text-decoration: none;
                color: inherit;
            }
            .cat-file:hover {
                color: inherit;
                background: #f4f4f4;
                text-decoration: none;
            }
            .cat-file img {
                height: 40px;
            }
            .cat-file div {
                display: flex;
                flex-direction: column;
                justify-content: center;
                align-items: flex-start;
                height: 100%;
            }
            .cat-file h6 {
                margin-bottom: 4px;
            }
            .cat-file small {
                line-height: 1;
                font-size: 0.8rem;
            }
            /* Custom styles for disclaimer modal */
            .disclaimer-modal .modal-content {
                border-radius: 8px;
            }
            .disclaimer-modal .modal-header {
                background: #f8f9fa;
                border-bottom: 1px solid #dee2e6;
            }
            .disclaimer-modal .alert-warning {
                border-left: 4px solid #ffc107;
            }
            .disclaimer-modal .btn-primary {
                background-color: #334b35;
                border-color: #334b35;
            }
            .disclaimer-modal .btn-primary:hover {
                background-color: #2a3d2c;
                border-color: #2a3d2c;
            }
            @media (min-width: 992px) {
                .cat-files-grid {
                    display: grid;
                    grid-template-columns: repeat(3, 1fr);
                    gap: 12px;
                }
            }
            @media (max-width: 576px) {
                .cat-files-grid {
                    display: grid;
                    grid-template-columns: repeat(1, 1fr);
                    gap: 12px;
                }
                .cat-sidebar {
                    border-bottom: 1px solid #dee2e6 !important;
                    padding-bottom: 24px;
                    margin-bottom: 24px;
                }
            }
        </style>
